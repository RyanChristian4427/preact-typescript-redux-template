{
  "name": "preact-typescript-redux-template",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/RyanChristian4427/preact-typescript-redux-template",
  "repository": "https://github.com/RyanChristian4427/preact-typescript-redux-template",
  "scripts": {
    "serve:dev": "preact watch -p 3000 --refresh",
    "serve:prod": "yarn build && sirv build -p 3000 --single",
    "build": "preact build --no-prerender",
    "build:analyze": "yarn build --no-esm --analyze",
    "lint": "yarn eslint --fix && yarn prettier --write",
    "lint:ci": "yarn eslint && yarn prettier",
    "eslint": "eslint 'src/**/*.{js,ts,tsx}' '*.js'",
    "prettier": "prettier 'src/**/*.{css,scss,json}' '*.json'",
    "postinstall": "node redux-persist-postinstall.mjs"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "bulma": "^0.9.0",
    "preact": "^10.4.4",
    "preact-feather": "^4.1.0",
    "preact-router": "^3.2.1",
    "react-redux": "^7.2.0",
    "redux-persist": "^6.0.0",
    "ts-api-toolkit": "^2.0.7"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "preact-cli": "^3.0.0-rc.14",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "sirv-cli": "^1.0.0-next.9",
    "typescript": "^3.9.5"
  }
}
